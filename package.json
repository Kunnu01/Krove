{
  "name": "krove",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": " next start",
    "lint": "next lint",
    "test": "TZ=UTC jest",
    "db:generate-prisma": "prisma generate",
    "db:migrate": "prisma migrate dev --create-only",
    "db:up": "prisma migrate deploy",
    "db:reset": "yarn prisma migrate reset --force --skip-seed",
    "db:view": "yarn prisma studio",
    "graphql:emit-schema": "ts-node --project tsconfig.json scripts/emit-schema.ts",
    "graphql:generate-client": "graphql-codegen --config codegen.ts",
    "sync": "yarn db:up && yarn db:generate-prisma && yarn graphql:emit-schema && yarn graphql:generate-client"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@genql/cli": "^6.3.2",
    "@genql/runtime": "^2.10.0",
    "@prisma/client": "^5.10.2",
    "dotenv": "^16.4.5",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-yoga": "^5.1.1",
    "next": "14.1.2",
    "next-auth": "^4.24.6",
    "react": "^18",
    "react-dom": "^18",
    "reflect-metadata": "^0.2.1",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "type-graphql": "2.0.0-beta.2"
  },
  "prisma": {
    "schema": "src/db/schema.prisma",
    "seed": "ts-node --transpile-only src/db/seed.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@emotion/react": "^11.11.4",
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/graphql-fields": "^1.3.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typegraphql-prisma": "^0.27.2",
    "typescript": "^5"
  }
}
